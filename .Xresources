Emacs.fontBackend: xft
Emacs.font: DejaVu Sans Mono-9
Emacs.verticalScrollBars: off
Emacs.menuBar: off
Emacs.toolBar: off

Xft.dpi:        96
Xft.antialias:  1
Xft.hinting:    4
Xft.hintstyle:  hintfull
Xft.rgba:       rgb
Xft.lcdfilter:  lcddefault

URxvt*font:        xft:ubuntu mono:size=11:style=medium:antialias=true
URxvt*boldFont:    xft:ubuntu mono:size=11:style=bold:antialias=true
URxvt.scrollBar: false
URxvt*foreground: White
URxvt*background: Black
